{
  "compilerOptions": {
    "target": "es6",
    "module": "NodeNext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": true,
    "inlineSourceMap": true,
    "baseUrl": ".",
    "sourceRoot": ".",
    "noEmit": true
  },
  "include": ["../src", "."],
}
