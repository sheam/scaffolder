{
  "name": "simple-code-templates",
  "version": "1.0.0",
  "description": "multi-file scaffolding for any type of project",
  "exports": "./dist/index.mjs",
  "type": "module",
  "engines": {
    "node": ">=14.16"
  },
  "bin": {
    "scaffolder": "./dist/index.mjs"
  },
  "scripts": {
    "compile": "tsc",
    "compile-watch": "tsc --watch",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --config=jest.config.mjs src",
    "test-run-dry": "cross-env DEBUG=TRUE node dist/index.mjs  src/testdir/dir1/subdir1 --name=MyComponent --template=simple --dryRun=true",
    "test-run-interactive-dry": "cross-env DEBUG=TRUE node dist/index.mjs --dryRun=true --name=MyComponent",
    "test-run": "cross-env DEBUG=TRUE node dist/index.mjs src/testdir/dir1/subdir1 --name=MyComponent --template=component",
    "clean": "npx rimraf --rf dist"
  },
  "keywords": [
    "scaffolding"
  ],
  "author": "Shea Martin",
  "license": "UNLICENSED",
  "devDependencies": {
    "@types/inquirer": "^9.0.1",
    "@types/inquirer-fuzzy-path": "^2.3.5",
    "@types/jest": "^29.0.3",
    "@types/node": "^18.7.18",
    "cross-env": "^7.0.3",
    "jest": "^29.0.3",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.1",
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "inquirer": "^9.1.1",
    "inquirer-fuzzy-path": "^2.3.0",
    "node-cli-arg-by-config": "^0.11.0",
    "velocityjs": "^2.0.6"
  }
}
